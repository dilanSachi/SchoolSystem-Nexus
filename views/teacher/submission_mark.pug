              span Subjects
          li.invisible
            a(href='#')
              i.fa.fa-smile-o
              span About Me
          li.invisible
            a(href='#')
              i.fa.fa-bell
              span Notification
          li.invisible
            a(href='#')
              i.fa.fa-envelope
              span Messaging
          li
            a(href='#')
              i.fa.fa-bars
      
    section(style = 'margin=10px')
      user#user
        section
          img(src='/images/userpic/Emma Watson.jpg')
          section(style = 'margin=10px')
            name Emma Watson
            actions
              a(href='#settings') settings
              |  
              a(href='#logout') logout
        br
        br
        br
        br


    #particles-js(ng-class="{'blur-view': showModal, 'particles-semi-transparent': viewIsCreationsOrPress}", style = "height : 150%")
      canvas.particles-js-canvas-el(style='width: 100%; height: 200%;', width='1910', height='3000')
    // ngView:
    .ng-scope(ng-view='', ng-class="{'blur-view': showPlayerView || showModal || showQuoteView}", style='')
      .introduction.ng-scope
    #titles(style = "top:5%;width : 100%").ng-scope
      h1(style = "margin-left:70px")= assignment.title
    #titles(style = "top:15%;width: 80%;margin-left:150px").ng-scope
      .tbl-content(style = "height:200px")
        table(cellpadding='0', cellspacing='0', border='0')
          tbody
            tr
              td(style= "color:#fff; text-decoration: none; font-size: 1.25em")= assignment.body_assignment
  
    #titles(style = "top:55%;width: 80%;margin-left:150px").ng-scope
      .tbl-content(style = "height:200px")
        table(cellpadding='0', cellspacing='0', border='0')
          tbody
            tr
              td(style= "color:#fff; text-decoration: none; font-size: 1.25em")= submission.assignment_submission

    #titles(style = "top:110%;width:100%").ng-scope
      // <canvas id="canvas" class="title-animated" ng-show="loaded"></canvas>
      .signup__container
        //.container__child.signup__thumbnail
        //  .signup__overlay
        .container__child.signup__form
          form(method='POST', action='/assignments/add')
            .form-group
              label(for='marks_allocated') Marks Allocated
              input.form-control(type='text', name='marks_allocated', placeholder='marks', required='')
            .form-group
              label(for='body_assignment') Comments
              textarea.form-control(name = 'body_assignment', placeholder='Assignment details',rows='4',  required  = '')
        
                    
