  body
    // particles.js container
    script(type="text/javascript", language="JavaScript").
      initializeClock('clockdiv', new Date(Date.parse(new Date()) + 15 * 24 * 60 * 60 * 1000));
    header
      nav#mainnav(role='navigation')
        ul
          li.invisible
            a(href='#')
              i.fa.fa-home
              span Home
          li.invisible
            a(href='#')
              i.fa.fa-th-large
              span Dashboard
          li.invisible
            a(href='#')
